 "format_data": "[{\"fieldname\": \"print_heading_template\", \"fieldtype\": \"Custom HTML\", \"options\": \"<div class=\\\"print-heading\\\">\\t\\t\\t\\t<h2><div>Academic Evaluation</div><br><small class=\\\"sub-heading\\\">{{ doc.date }}</small>\\t\\t\\t\\t</h2></div>\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"evaluatee_party_name\", \"print_hide\": 0, \"label\": \"Party Name\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"academic_rank\", \"print_hide\": 0, \"label\": \"Academic Rank\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"academic_year\", \"print_hide\": 0, \"label\": \"Academic Year\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"template\", \"print_hide\": 0, \"label\": \"Template\"}, {\"fieldname\": \"evaluation_details\", \"print_hide\": 0, \"label\": \"Evaluation Details\", \"visible_columns\": [{\"fieldname\": \"criterion\", \"print_width\": \"50%\", \"print_hide\": 0}, {\"fieldname\": \"evaluation\", \"print_width\": \"50%\", \"print_hide\": 0}]}, {\"fieldname\": \"notes\", \"print_hide\": 0, \"label\": \"Notes\"}]",
 "html": "<!DOCTYPE html>\r\n<html>\r\n<head>\r\n    <style>\r\n        /* \u0625\u0639\u0627\u062f\u0629 \u062a\u0639\u064a\u064a\u0646 \u0627\u0644\u0625\u0639\u062f\u0627\u062f\u0627\u062a \u0627\u0644\u0627\u0641\u062a\u0631\u0627\u0636\u064a\u0629 */\r\n        * {\r\n            margin: 0;\r\n            padding: 0;\r\n            box-sizing: border-box;\r\n        }\r\n\r\n        /* \u062a\u062d\u062f\u064a\u062f \u0627\u0644\u062e\u0637 \u0627\u0644\u0623\u0633\u0627\u0633\u064a */\r\n        body {\r\n            font-family: 'Calibri', sans-serif; /* \u062a\u0639\u064a\u064a\u0646 \u062e\u0637 Calibri */\r\n            margin: 0;\r\n            padding: 0;\r\n        }\r\n\r\n        /* \u062a\u0646\u0633\u064a\u0642 \u0627\u0644\u0647\u064a\u062f\u0631 */\r\n        .header {\r\n            display: flex;\r\n            justify-content: space-between;\r\n            align-items: center;\r\n            border-bottom: 2px solid #000;\r\n            padding: 0 0 10px 0; /* \u062a\u0639\u064a\u064a\u0646 padding \u0644\u0644\u0623\u0639\u0644\u0649 0 \u0648\u0644\u0644\u0623\u0633\u0641\u0644 10 \u0628\u0643\u0633\u0644 */\r\n            margin: 0; /* \u0625\u0632\u0627\u0644\u0629 \u0623\u064a \u0647\u0627\u0645\u0634 \u0625\u0636\u0627\u0641\u064a */\r\n        }\r\n\r\n        .header .text-en, .header .text-ar {\r\n            font-weight: bold; /* \u062a\u0639\u064a\u064a\u0646 \u0627\u0644\u0646\u0635 \u0644\u064a\u0643\u0648\u0646 \u063a\u0627\u0645\u0642 */\r\n        }\r\n\r\n        .header .text-en {\r\n            text-align: left;\r\n            flex: 1;\r\n            direction: ltr; /* \u0636\u0628\u0637 \u0627\u062a\u062c\u0627\u0647 \u0627\u0644\u0646\u0635 \u0627\u0644\u0625\u0646\u062c\u0644\u064a\u0632\u064a */\r\n        }\r\n\r\n        .header .text-ar {\r\n            text-align: right;\r\n            flex: 1;\r\n            direction: rtl; /* \u0636\u0628\u0637 \u0627\u062a\u062c\u0627\u0647 \u0627\u0644\u0646\u0635 \u0627\u0644\u0639\u0631\u0628\u064a */\r\n        }\r\n\r\n        .header img {\r\n            width: 100px; /* \u062a\u0639\u062f\u064a\u0644 \u0627\u0644\u062d\u062c\u0645 \u062d\u0633\u0628 \u0627\u0644\u062d\u0627\u062c\u0629 */\r\n            height: auto;\r\n        }\r\n\r\n        .separator {\r\n            border-top: 2px solid #000;\r\n            margin: 0; /* \u0625\u0632\u0627\u0644\u0629 \u0627\u0644\u0645\u0633\u0627\u0641\u0627\u062a \u0627\u0644\u0625\u0636\u0627\u0641\u064a\u0629 */\r\n            padding: 0; /* \u0625\u0632\u0627\u0644\u0629 \u0623\u064a \u062d\u0634\u0648 \u0625\u0636\u0627\u0641\u064a */\r\n        }\r\n\r\n        .section {\r\n            margin: 5px 0; /* \u0647\u0627\u0645\u0634 \u0639\u0644\u0648\u064a \u0648\u0633\u0641\u0644\u064a */\r\n        }\r\n        \r\n        .section p {\r\n            text-align: right; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0639\u0646\u0648\u0627\u0646 \u0644\u0644\u064a\u0645\u064a\u0646 */\r\n            direction: rtl; /* \u062c\u0639\u0644 \u0627\u0644\u0639\u0646\u0648\u0627\u0646 \u063a\u0627\u0645\u0642 */\r\n            font-size: 18px;\r\n        }\r\n        \r\n        .section h2 {\r\n            text-align: right; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0639\u0646\u0648\u0627\u0646 \u0644\u0644\u064a\u0645\u064a\u0646 */\r\n            font-weight: bold; /* \u062c\u0639\u0644 \u0627\u0644\u0639\u0646\u0648\u0627\u0646 \u063a\u0627\u0645\u0642 */\r\n        }\r\n\r\n        .details {\r\n            margin: 10px 0;\r\n            text-align: right; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0646\u0635 \u0644\u0644\u064a\u0645\u064a\u0646 \u0641\u064a \u0642\u0633\u0645 \u0627\u0644\u0645\u0644\u0627\u062d\u0638\u0627\u062a */\r\n        }\r\n\r\n        .details label {\r\n            font-weight: bold;\r\n        }\r\n\r\n        .date-section {\r\n            display: flex;\r\n            justify-content: space-between; /* \u062a\u0648\u0632\u064a\u0639 \u0627\u0644\u0645\u0633\u0627\u0641\u0627\u062a \u0628\u064a\u0646 \u0627\u0644\u0639\u0646\u0627\u0635\u0631 \u0628\u0634\u0643\u0644 \u0645\u062a\u0633\u0627\u0648\u064a */\r\n            margin: 10px 0; /* \u0647\u0627\u0645\u0634 \u0639\u0644\u0648\u064a \u0648\u0633\u0641\u0644\u064a \u0623\u0642\u0644 */\r\n            font-size: 14px; /* \u062a\u0635\u063a\u064a\u0631 \u062d\u062c\u0645 \u0627\u0644\u062e\u0637 */\r\n        }\r\n\r\n        .date-section .date, .date-section .date-en {\r\n            text-align: center; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0646\u0635 \u0641\u064a \u0627\u0644\u0645\u0631\u0643\u0632 */\r\n            width: 48%; /* \u0639\u0631\u0636 \u0645\u062a\u0633\u0627\u0648\u064a \u0645\u0639 \u0627\u0644\u0645\u0633\u0627\u062d\u0629 \u0627\u0644\u0645\u062d\u064a\u0637\u0629 */\r\n        }\r\n\r\n        .date-section .date {\r\n            text-align: left; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0646\u0635 \u0644\u0644\u064a\u0633\u0627\u0631 \u0644\u0644\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0625\u0646\u062c\u0644\u064a\u0632\u064a */\r\n        }\r\n\r\n        .date-section .date-en {\r\n            text-align: right; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0646\u0635 \u0644\u0644\u064a\u0633\u0627\u0631 \u0644\u0644\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0625\u0646\u062c\u0644\u064a\u0632\u064a */\r\n            direction: ltr; /* \u0636\u0628\u0637 \u0627\u062a\u062c\u0627\u0647 \u0627\u0644\u0646\u0635 \u0627\u0644\u0625\u0646\u062c\u0644\u064a\u0632\u064a */\r\n        }\r\n\r\n        .title {\r\n            text-align: center;\r\n            font-weight: bold;\r\n            font-size: 24px; /* \u062d\u062c\u0645 \u0627\u0644\u062e\u0637 \u0627\u0644\u0643\u0628\u064a\u0631 */\r\n            margin-top: 20px; /* \u0647\u0627\u0645\u0634 \u0639\u0644\u0648\u064a \u0644\u0644\u0641\u0635\u0644 */\r\n            margin-bottom: 10px; /* \u062a\u0642\u0644\u064a\u0644 \u0627\u0644\u0647\u0627\u0645\u0634 \u0627\u0644\u0633\u0641\u0644\u064a */\r\n        }\r\n\r\n        .info-section {\r\n            display: flex;\r\n            flex-direction: row; /* \u062a\u0631\u062a\u064a\u0628 \u0627\u0644\u0639\u0646\u0627\u0635\u0631 \u0645\u0646 \u0627\u0644\u064a\u0633\u0627\u0631 \u0644\u0644\u064a\u0645\u064a\u0646 */\r\n            justify-content: space-between; /* \u062a\u0648\u0632\u064a\u0639 \u0627\u0644\u0645\u0633\u0627\u0641\u0627\u062a \u0628\u064a\u0646 \u0627\u0644\u0639\u0646\u0627\u0635\u0631 \u0628\u0634\u0643\u0644 \u0645\u062a\u0633\u0627\u0648\u064a */\r\n            margin: 10px 0; /* \u062a\u0642\u0644\u064a\u0644 \u0627\u0644\u0647\u0627\u0645\u0634 \u0627\u0644\u0639\u0644\u0648\u064a \u0648\u0627\u0644\u0633\u0641\u0644\u064a */\r\n            direction: rtl; /* \u0627\u062a\u062c\u0627\u0647 \u0627\u0644\u0646\u0635 \u0645\u0646 \u0627\u0644\u064a\u0645\u064a\u0646 \u0644\u0644\u064a\u0633\u0627\u0631 */\r\n            margin-bottom: 20px;\r\n        }\r\n\r\n        .info-section .info {\r\n            display: flex;\r\n            flex-direction: row; /* \u062a\u0631\u062a\u064a\u0628 \u0627\u0644\u0639\u0646\u0627\u0635\u0631 \u0641\u064a \u0646\u0641\u0633 \u0627\u0644\u0633\u0637\u0631 */\r\n            align-items: center;\r\n            font-size: 18px; /* \u062a\u0643\u0628\u064a\u0631 \u062d\u062c\u0645 \u0627\u0644\u062e\u0637 */\r\n            width: 100%; /* \u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u0627\u0644\u0639\u0631\u0636 \u0627\u0644\u0643\u0627\u0645\u0644 \u0627\u0644\u0645\u062a\u0627\u062d */\r\n        }\r\n\r\n        .info-section .info > div {\r\n            text-align: right; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0646\u0635 \u0644\u0644\u064a\u0645\u064a\u0646 \u062f\u0627\u062e\u0644 \u0627\u0644\u0639\u0646\u0627\u0635\u0631 */\r\n            flex: 1; /* \u062a\u0648\u0632\u064a\u0639 \u0627\u0644\u0645\u0633\u0627\u062d\u0629 \u0628\u0627\u0644\u062a\u0633\u0627\u0648\u064a \u0628\u064a\u0646 \u0627\u0644\u0639\u0646\u0627\u0635\u0631 */\r\n            margin: 0; /* \u0625\u0632\u0627\u0644\u0629 \u0627\u0644\u0645\u0633\u0627\u0641\u0629 \u0628\u064a\u0646 \u0627\u0644\u0639\u0646\u0627\u0635\u0631 */\r\n        }\r\n\r\n        .info-section .info > div:first-child {\r\n            margin-right: 0; /* \u0625\u0632\u0627\u0644\u0629 \u0627\u0644\u0645\u0633\u0627\u0641\u0629 \u0639\u0644\u0649 \u0627\u0644\u064a\u0645\u064a\u0646 \u0644\u0623\u0648\u0644 \u0639\u0646\u0635\u0631 */\r\n        }\r\n\r\n        .info-section .info span {\r\n            font-weight: bold; /* \u062c\u0639\u0644 \u0627\u0644\u0646\u0635 \u063a\u0627\u0645\u0642 */\r\n        }\r\n\r\n        table {\r\n            width: 100%;\r\n            border-collapse: collapse;\r\n            direction: rtl; /* \u0627\u062a\u062c\u0627\u0647 \u0627\u0644\u0646\u0635 \u0645\u0646 \u0627\u0644\u064a\u0645\u064a\u0646 \u0644\u0644\u064a\u0633\u0627\u0631 */\r\n            margin-bottom: 15px;\r\n            margin-top: 10px;\r\n        }\r\n\r\n        th, td {\r\n            border: 1px solid #000;\r\n            padding: 8px;\r\n            text-align: center;\r\n        }\r\n\r\n        .signatures {\r\n            margin-top: 40px; /* \u0625\u0636\u0627\u0641\u0629 \u0647\u0627\u0645\u0634 \u0639\u0644\u0648\u064a \u0644\u0644\u0641\u0635\u0644 */\r\n            text-align: right; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0646\u0635 \u0644\u0644\u064a\u0645\u064a\u0646 */\r\n            direction: rtl;\r\n        }\r\n\r\n        .signatures p {\r\n            font-size: 18px; /* \u062a\u0643\u0628\u064a\u0631 \u062d\u062c\u0645 \u0627\u0644\u062e\u0637 */\r\n            margin: 0; /* \u0625\u0632\u0627\u0644\u0629 \u0627\u0644\u0645\u0633\u0627\u0641\u0629 \u0628\u064a\u0646 \u0627\u0644\u0641\u0642\u0631\u0627\u062a */\r\n            direction: rtl;\r\n        }\r\n\r\n        .signatures .line {\r\n            border-top: 1px dashed #000;\r\n            display: inline-block;\r\n            width: 200px; /* \u0639\u0631\u0636 \u0627\u0644\u062e\u0637 \u0627\u0644\u0645\u062a\u0642\u0637\u0639 \u062d\u0633\u0628 \u0627\u0644\u062d\u0627\u062c\u0629 */\r\n            margin-left: 4px; /* \u0625\u0636\u0627\u0641\u0629 \u0645\u0633\u0627\u0641\u0629 \u0628\u064a\u0646 \u0627\u0644\u0646\u0635 \u0648\u0627\u0644\u062e\u0637 */\r\n            vertical-align: middle; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u062e\u0637 \u0645\u0639 \u0627\u0644\u0646\u0635 \u0639\u0645\u0648\u062f\u064a\u064b\u0627 */\r\n        }\r\n\r\n        .signatures .section {\r\n            display: flex;\r\n            justify-content: space-between; /* \u062a\u0648\u0632\u064a\u0639 \u0627\u0644\u0645\u0633\u0627\u0641\u0627\u062a \u0628\u064a\u0646 \u0627\u0644\u0639\u0646\u0627\u0635\u0631 \u0628\u0634\u0643\u0644 \u0645\u062a\u0633\u0627\u0648\u064a */\r\n        }\r\n\r\n        .signatures .section div {\r\n            text-align: right; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0646\u0635 \u0644\u0644\u064a\u0645\u064a\u0646 */\r\n            text-align: center; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u0646\u0635 \u0641\u064a \u0627\u0644\u0645\u0631\u0643\u0632 */\r\n            width: 48%; \r\n        }\r\n\r\n        .signatures .section .name {\r\n            display: flex;\r\n            align-items: center; /* \u0645\u062d\u0627\u0630\u0627\u0629 \u0627\u0644\u062e\u0637 \u0645\u0639 \u0627\u0644\u0646\u0635 \u0639\u0645\u0648\u062f\u064a\u064b\u0627 */\r\n        }\r\n    </style>\r\n</head>\r\n<body>\r\n    <div class=\"header\">\r\n        <div class=\"text-en\">\r\n            Republic of Yemen<br>\r\n            Sana'a University<br>\r\n            General Administration<br>\r\n            For Academic Affairs\r\n        </div>\r\n        <div class=\"logo\">\r\n            <!-- \u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u0645\u0633\u0627\u0631 \u0627\u0644\u0635\u0648\u0631\u0629 \u0627\u0644\u0645\u062d\u062f\u062f -->\r\n            <img src=\"/files/Uni.jpg\" alt=\"University Logo\">\r\n        </div>\r\n        <div class=\"text-ar\">\r\n            \u0627\u0644\u062c\u0645\u0647\u0648\u0631\u064a\u0629 \u0627\u0644\u064a\u0645\u0646\u064a\u0629<br>\r\n            \u062c\u0627\u0645\u0639\u0629 \u0635\u0646\u0639\u0627\u0621<br>\r\n            \u0627\u0644\u0625\u062f\u0627\u0631\u0629 \u0627\u0644\u0639\u0627\u0645\u0629 \u0644\u0644\u0634\u0624\u0648\u0646 \u0627\u0644\u0623\u0643\u0627\u062f\u064a\u0645\u064a\u0629<br>\r\n            \u0648\u0634\u0624\u0648\u0646 \u0623\u0639\u0636\u0627\u0621 \u0647\u064a\u0626\u0629 \u0627\u0644\u062a\u062f\u0631\u064a\u0633<br><br> <!-- \u0625\u0636\u0627\u0641\u0629 \u0633\u0637\u0631 \u0641\u0627\u0631\u063a -->\r\n        </div>\r\n    </div>\r\n\r\n    <!-- \u0641\u0627\u0635\u0644 \u0633\u0637\u0631 \u0648\u0627\u062d\u062f \u0641\u0642\u0637 -->\r\n    <div class=\"separator\"></div>\r\n\r\n   <!-- \u0642\u0633\u0645 \u0627\u0644\u062a\u0627\u0631\u064a\u062e -->\r\n    <div class=\"date-section\">\r\n        <div class=\"date\">\r\n           Date: {{ doc.date }}\r\n        </div>\r\n        <div class=\"date-en\">\r\n            {{ doc.date }} :\u0627\u0644\u062a\u0627\u0631\u064a\u062e \r\n        </div>\r\n    </div>\r\n\r\n    <!-- \u0639\u0646\u0648\u0627\u0646 \u0643\u0628\u064a\u0631 \u0628\u062e\u0637 \u063a\u0627\u0645\u0642 -->\r\n    <div class=\"title\">\r\n        \u062a\u0642\u0631\u064a\u0631 \u0639\u0646 \u0627\u0644\u0623\u062f\u0627\u0621 \u0627\u0644\u0623\u0643\u0627\u062f\u064a\u0645\u064a \u0644\u0639\u0636\u0648 \u0647\u064a\u0626\u0629 \u0627\u0644\u062a\u062f\u0631\u064a\u0633\r\n    </div>\r\n\r\n    <!-- \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u0625\u0636\u0627\u0641\u064a\u0629 \u0641\u064a \u0646\u0641\u0633 \u0627\u0644\u0633\u0637\u0631 -->\r\n    <div class=\"info-section\">\r\n        <div class=\"info\">\r\n            <div><span>\u0627\u0644\u0627\u0633\u0645:</span> {{ doc.evaluatee_party_name }}</div>\r\n            <div><span>\u0627\u0644\u062f\u0631\u062c\u0629 \u0627\u0644\u0639\u0644\u0645\u064a\u0629:</span> {{ doc.academic_rank }}</div>\r\n            <div><span>\u0627\u0644\u0639\u0627\u0645 \u0627\u0644\u0623\u0643\u0627\u062f\u064a\u0645\u064a:</span> {{ doc.academic_year }}</div>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- \u062a\u062d\u062f\u064a\u062f \u0627\u0633\u0645 \u0642\u0627\u0644\u0628 \u0627\u0644\u062a\u0642\u064a\u064a\u0645 -->\r\n    <div class=\"section\">\r\n        <p>\u062a\u0641\u0627\u0635\u064a\u0644 \u0627\u0644\u062a\u0642\u064a\u064a\u0645</p>\r\n        <p>\u0642\u0627\u0644\u0628 \u0627\u0644\u062a\u0642\u064a\u064a\u0645: {{ doc.template }}</p>\r\n    </div>\r\n\r\n    <!-- \u062c\u062f\u0648\u0644 \u0627\u0644\u062a\u0642\u064a\u064a\u0645 -->\r\n    <div class=\"section\">\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th>\u0627\u0644\u0631\u0642\u0645</th>\r\n                    <th>\u0627\u0644\u0645\u0639\u064a\u0627\u0631</th>\r\n                    <th>\u0627\u0644\u062a\u0642\u064a\u064a\u0645</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                {% for row in doc.evaluation_details %}\r\n                <tr>\r\n                    <td>{{ loop.index }}</td>\r\n                    <td>{{ row.criterion }}</td>\r\n                    <td>\r\n                        {% if row.evaluation == 'Excellent' %}\r\n                            \u0645\u0645\u062a\u0627\u0632\r\n                        {% elif row.evaluation == 'Good' %}\r\n                            \u062c\u064a\u062f \u062c\u062f\u064b\u0627\r\n                        {% elif row.evaluation == 'Satisfactory' %}\r\n                            \u062c\u064a\u062f\r\n                        {% elif row.evaluation == 'Fair' %}\r\n                            \u0645\u0642\u0628\u0648\u0644\r\n                        {% elif row.evaluation == 'Poor' %}\r\n                            \u0636\u0639\u064a\u0641\r\n                        {% endif %}\r\n                    </td>\r\n                </tr>\r\n                {% endfor %}\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n\r\n    <!-- \u0645\u0644\u0627\u062d\u0638\u0627\u062a -->\r\n    <div class=\"section\">\r\n        <p>\u0645\u0644\u0627\u062d\u0638\u0627\u062a:</p>\r\n         <p> {{ doc.notes }} </p> \r\n        \r\n    </div>\r\n\r\n    <!-- \u062a\u0648\u0642\u064a\u0639\u0627\u062a -->\r\n    <div class=\"signatures\">\r\n        <div class=\"section\">\r\n            <div class=\"name\">\r\n                <p>\u0631\u0626\u064a\u0633 \u0627\u0644\u0642\u0633\u0645:</p>\r\n                <p class=\"line\"></p>\r\n            </div>\r\n            <div class=\"name\">\r\n                <p>\u0639\u0645\u064a\u062f \u0627\u0644\u0643\u0644\u064a\u0629:</p>\r\n                <p class=\"line\"></p>\r\n            </div>\r\n        </div>\r\n        <div class=\"section\">\r\n            <div>\r\n                <p>\u0627\u0644\u062a\u0648\u0642\u064a\u0639:</p>\r\n            </div>\r\n            <div>\r\n                <p>\u0627\u0644\u062a\u0648\u0642\u064a\u0639:</p>\r\n            </div>\r\n        </div>\r\n        <div class=\"section\">\r\n            <div>\r\n                <p>\u0627\u0644\u062a\u0627\u0631\u064a\u062e:</p>\r\n            </div>\r\n            <div>\r\n                <p>\u0627\u0644\u062a\u0627\u0631\u064a\u062e:</p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</body>\r\n</html>\r\n",
