  "footer": "{% set council_head = \"x\" %}\n{% set council_reporter = None %}\n    \n    \n    \n\n<hr>\n<table style=\"width:100%;\">\n    <tbody style=\"text-align:center;\">\n        <tr>\n            <td><b>مقرر المجلس</b></td>\n            <td><b>رئيس المجلس</b></td>\n        </tr>\n        <tr>\n            <td>\n                {% for member in doc.members %}\n                    {% if member.member_role == _(\"Council Head\") %}\n                        {{member.member_name}}\n                    {% endif %}\n                {% endfor %}\n            </td>\n            <td>\n                {% for member in doc.members %}\n                    {% if member.member_role == _(\"Council Reporter\") %}\n                         {{ member.member_name }}\n                    {% endif %}\n                {% endfor %}\n            </td>\n        </tr>\n        <tr>\n            <td><br><br>..........................</td>\n            <td><br><br>..........................</td>\n        </tr>\n    </tbody>\n</table>\n<hr>\n<p style=\"font-size:16px;\"><b>\n    الترميز:\n     {% if doc.minute_hash %}\n        {{ doc.minute_hash }}\n    {% else %}\n        No hash available.\n    {% endif %}\n</b></p>",
